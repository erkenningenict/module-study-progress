		"primereact": "3.1.8",
		"primeicons": "2.0.0",
		"react": "16.8.6",
		"react-dom": "16.8.6",
		"react-scripts": "3.1.0",
		"rewire": "^4.0.1",
		"typescript": "3.5.3"
	},
	"devDependencies": {
		"@erkenningen/build": "1.0.7",
		"@reach/router": "1.2.1",
		"@types/jest": "23.3.12",
		"@types/node": "10.12.18",
		"@types/react": "16.7.18",
		"@types/react-dom": "16.8.5",
		"archiver": "3.0.0",
		"classnames": "2.2.6",
		"connect": "3.6.6",
		"dotenv": "6.2.0",
		"eslint-plugin-prettier": "2.6.2",
		"fs-extra": "7.0.1",
		"gatsby": "2.13.50",
		"husky": "0.14.3",
		"ng-apimock": "1.4.7",
		"npm-run-all": "4.1.2",
		"prettier": "1.18.2",
		"pretty-quick": "1.6.0",
		"react-html-parser": "2.0.2",
		"react-router-dom": "5.0.1",
		"react-transition-group": "4.2.1",
		"source-map-explorer": "2.0.1",
		"tslint": "5.18.0",
		"tslint-config-prettier": "1.18.0",
		"tslint-eslint-rules": "5.4.0",
		"tslint-react": "4.0.0"
	},
	"scripts": {
		"start": "run-p -r mockserver start:app",
		"start:app": "react-scripts start",
		"build:dnn": "node ./node_modules/@erkenningen/build/scripts/create-dnn-module.js",
		"build:prod": "npm-run-all build build:dnn",
		"build:deploy": "node ./node_modules/@erkenningen/build/scripts/create-deploy-file.js",
		"build:netlify": "cp public/index.netlify.html public/index.html && node ./build.js",
		"build": "react-scripts build",
		"precommit": "pretty-quick --staged",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"mockserver": "node ./mocks/mocks.js",
		"analyze": "source-map-explorer 'build/static/js/*.js'"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"proxy": "http://localhost:4000",
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
